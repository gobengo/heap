  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via deno",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect",
        "--allow-net",
        "${file}"
      ],
      "port": 9229,
      "outputCapture": "std",
      // "sourceMaps": true
    },
    {
      "name": "Launch via deno test",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "test",
        "--inspect",
        "--allow-net",
        "${file}"
      ],
      "port": 9229,
      "outputCapture": "std",
      // "sourceMaps": true
    }
  ]
}
